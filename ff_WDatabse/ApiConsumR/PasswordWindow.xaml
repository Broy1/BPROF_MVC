<Window x:Class="ApiConsumR.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApiConsumR"
        mc:Ignorable="d"
        Title="PasswordWindow" Height="235" Width="221">
    <StackPanel>
        <Label Content="Username" Background="LightGreen" Margin="10"/>
        <TextBox x:Name="tb_username" Margin="10"/>
        <Label Content="Password" Background="LightGreen" Margin="10"/>
        <PasswordBox x:Name="tb_pass" Margin="10"/>
        <Button Content="Login" Margin="10" Click="Button_Click"/>
    </StackPanel>
</Window>
